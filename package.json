  "dependencies": {
    "socket.io": "0.9.x",
    "colors": "0.x.x",
    "pkginfo": "0.2.x",
    "optimist": "0.3.x",
    "restify": "2.x.x",
    "underscore": "1.5.x",
    "node-zookeeper-client": "0.2.x",
    "redis": "0.10.x",
    "mongoose": "~3.8.x",
    "node-gcm": "0.9.x",
    "shortid": "~2.0.0",
    "async": "~0.2.9",
    "iodocs": "~0.0.1",
    "supervisor": "~0.5.6",
    "groc": "~0.6.1"
  },
  "devDependencies": {
    "should": "~2.1.1"
  },
  "keywords": [
    "push",
    "websocket",
    "gcm",
    "apm",
    "xmpp",
    "api"
  ],
  "repository": "git://github.com/xpush/node-xpush",
  "main": "./lib/xpush.js",
  "bin": {
    "npush": "./bin/xpush"
  },
  "scripts": {
    "test": "node test",
    "dev-gateway": "node_modules/.bin/supervisor -e 'js|json' -- ./bin/xpush --port 8000 --config ./config.sample.json --gateway",
    "dev-api": "node_modules/.bin/supervisor -e 'js|json' -- ./bin/xpush --port 9000 --config ./config.sample.json",
    "api-ref": "cp ./doc/iodocs-config.json ./node_modules/iodocs/config.json && cp ./doc/apiconfig.json ./node_modules/iodocs/public/data/apiconfig.json && cp ./doc/xpush.json ./node_modules/iodocs/public/data/xpush.json && node ./node_modules/iodocs/app.js"
  },
  "engines": {
    "node": ">= 0.8.0"
  }
}
